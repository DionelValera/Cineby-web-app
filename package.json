{
  "name": "cineby",
  "version": "1.0.0",
  "description": "Cineby is a web app, an app of home theater and cine in home",
  "keywords": [
    "cine",
    "Cine",
    "home",
    "theater",
    "pelicula",
    "movie",
    "teatro"
  ],
  "homepage": "https://github.com/DionelValera/cineby-web-app#readme",
  "bugs": {
    "url": "https://github.com/DionelValera/cineby-web-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DionelValera/cineby-web-app.git"
  },
  "license": "ISC",
  "author": "Dionel Valera",
  "type": "commonjs",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.dionelvalera.cinebyapp",
    "productName": "Cineby Home Theater",
    "files": [
      "main.js",
      "preload.js",
      "package.json",
      "icon.png"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "icon.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "icon.png"
    }
  }
}
